<p id="notice"><%= notice %></p>

<h1>Listing Primary Stats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @primary_stats.each do |primary_stat| %>
      <tr>
        <td><%= primary_stat.name %></td>
        <td><%= link_to 'Show', primary_stat %></td>
        <td><%= link_to 'Edit', edit_primary_stat_path(primary_stat) %></td>
        <td><%= link_to 'Destroy', primary_stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Primary stat', new_primary_stat_path %>
