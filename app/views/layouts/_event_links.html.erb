<% if user_signed_in? %>
<li><a href="#"><i class="fa fa-calendar"></i>  Events</a>
	<ul class="list-unstyled">
	
	<% if current_user.admin? %>
		<%= content_tag :li, class: 'sub-nav' do %>
		<%= link_to 'View Events', admin_events_path %>
		<% end %>
	<% else %>
		<%= content_tag :li, class: 'sub-nav' do %>
		<%= link_to 'Coming soon...', '#' %>
		<% end %>
	<% end %>

	</ul>
</li>
<% end %>