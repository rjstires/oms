		<%= simple_form_for([:admin, @character]) do |f| %>
		    <div class="form-inputs">
		    <%= f.input :spec %>
		    <%= f.association :classification %>
		    <%= f.association :armor_type %>
		    <%= f.association :tier_token %>
		    <%= f.association :primary_stat %>
		  </div>
			<div class="btn-group">
				<%= f.button :submit, :class => "btn btn-primary btn-sm" %>
				<%= yield :buttons %>
			</div>
		<% end %>		