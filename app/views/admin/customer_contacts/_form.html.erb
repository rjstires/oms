<%= simple_form_for([:admin, @customer_contact]) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
	<%= f.input :ctype, :as => :select, :collection => CustomerContact.ctypes.keys.to_a %>
	<%= f.input :value %>
</div>

<div class="form-actions">
	<%= f.button :submit %>
</div>
<% end %>
