<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="col-md-12">
      <table class="table table-striped table-hover">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
