<p id="notice"><%= notice %></p>

<h1>Listing Team Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_statuses.each do |team_status| %>
      <tr>
        <td><%= team_status.name %></td>
        <td><%= link_to 'Show', team_status %></td>
        <td><%= link_to 'Edit', edit_team_status_path(team_status) %></td>
        <td><%= link_to 'Destroy', team_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team status', new_team_status_path %>
